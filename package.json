{
  "name": "koa-vue-boilerplate",
  "version": "1.0.0",
  "description": "koa vue boilerplat",
  "keywords": [
    "web",
    "app",
    "http",
    "application",
    "framework",
    "middleware",
    "koa",
    "vue"
  ],
  "main": "index.js",
  "scripts": {
    "init": "npm install --no-optional -s && pm2 install pm2-intercom",
    "lint": "eslint --ext .js,.vue app --fix --cache",
    "dev": "cross-env HOT=hot NODE_ENV=development node ./build/dev.js",
    "debug": "cross-env HOT=hot NODE_ENV=development node  --inspect-brk build/dev.js",
    "debug:watch": "npm run clean && cross-env NODE_ENV=development node --inspect-brk ./build/watch.js",
    "dev:watch": "npm run clean && cross-env NODE_ENV=development node ./build/watch.js",
    "build": "npm run clean && easy -c --devtool nosources-source-map -f build/client.js build prod && easy -f build/server.js build prod",
    "build:test": "npm run clean && easy -f build/client.js build test && easy -f build/server.js build test",
    "start": "npm run clean && npm run build && pm2 start ./pm2.config.js --env=production",
    "restart": "pm2 restart pm2.config.js --env=production",
    "stop": "pm2 stop app",
    "clean": "rimraf ./logs/* && rimraf ./public/* && rimraf ./app/view/*",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "pre-commit": [
    "lint"
  ],
  "author": "jxcao",
  "license": "MIT",
  "dependencies": {
    "axios": "^0.18.0",
    "co": "^4.6.0",
    "cross-env": "^5.2.0",
    "debug": "^3.1.0",
    "deepmerge": "^2.1.1",
    "depd": "^1.1.2",
    "is-type-of": "^1.2.0",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-cors": "0.0.16",
    "koa-csrf": "^3.0.6",
    "koa-favicon": "^2.0.1",
    "koa-generic-session": "^2.0.1",
    "koa-helmet": "^4.0.0",
    "koa-limit": "^1.0.2",
    "koa-log4": "^2.3.0",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "log4js": "^3.0.5",
    "lru-cache": "^4.1.3",
    "mime": "^2.3.1",
    "mz": "^2.7.0",
    "node-notifier": "^5.2.1",
    "pm2": "^3.0.4",
    "rimraf": "^2.6.2",
    "serialize-javascript": "^1.5.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vue-server-renderer": "^2.5.17",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-assign": "^6.22.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-plugin-transform-runtime": "^6.15.0",
    "babel-preset-env": "^1.6.0",
    "chalk": "^2.4.1",
    "easywebpack-cli": "^3.11.3",
    "easywebpack-vue": "^4.1.2",
    "eslint": "^3.0.0",
    "eslint-config-airbnb-base": "^11.2.0",
    "eslint-config-vue": "^2.0.2",
    "eslint-plugin-html": "^3.1.1",
    "eslint-plugin-import": "^2.6.1",
    "eslint-plugin-vue": "^2.1.0",
    "imagemin-webpack-plugin": "^2.2.0",
    "kcors": "^2.2.2",
    "less": "^3.8.1",
    "less-loader": "^4.1.0",
    "nodemon": "^1.18.3",
    "pre-commit": "^1.2.2",
    "webpack": "^4.19.1",
    "webpack-dev-middleware": "^3.3.0",
    "webpack-hot-middleware": "^2.24.2"
  }
}
